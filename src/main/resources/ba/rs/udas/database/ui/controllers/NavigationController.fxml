<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane xmlns:fx="http://javafx.com/fxml"
  alignment="CENTER"
  hgap="10" vgap="10" fx:controller="ba.rs.udas.database.ui.controllers.NavigationController">
  <columnConstraints>
  </columnConstraints>

  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0"/>
    <RowConstraints minHeight="10.0" prefHeight="30.0"/>
    <RowConstraints/>
  </rowConstraints>

  <padding>
    <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
  </padding>

  <Button GridPane.rowIndex="0" id="membersBtn" mnemonicParsing="false"
    onAction="#onMembersBtnClicked"
    text="Pregled članova"/>

  <Button GridPane.rowIndex="1" id="artSchoolBtn" mnemonicParsing="false"
    onAction="#onArtSchoolBtnClicked"
    text="Škola slikanja"/>

  <Button GridPane.rowIndex="2" id="logoutBtn" mnemonicParsing="false"
    onAction="#onLogoutBtnClicked"
    text="Odjava"/>

</GridPane>
